        @section scripts {

@Scripts.Render("~/bundles/Chart2")

            <script type="text/javascript" src="http://cdn.datatables.net/1.10.2/js/jquery.dataTables.min.js"></script>
            <script type="text/javascript">
                //----Donut Chart---------
                var c = document.getElementById("DonutChart");
                var ctx = c.getContext("2d");
                var tData = $.getValues("/pitp/DonutChartData");
                var myPieChart = new Chart(ctx, {
                    type: 'doughnut',
                    responsive: true,
                    data: tData,
                });

                var bc = document.getElementById("BarChart");
                var btx = bc.getContext("2d");
                var dData = $.getValues("/pitp/BarChartData");
                var myPieChart = new Chart(btx, {
                    type: 'bar',
                    responsive: true,
                    data: dData,
                });

                $('#myTable').DataTable({
                    searching: false,
                    ordering: true
                });
            </script>
        }
